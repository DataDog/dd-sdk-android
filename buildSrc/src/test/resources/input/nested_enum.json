{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DateTime",
  "type": "object",
  "properties": {
    "date": {
      "type": "object",
      "properties": {
        "year": {
          "type" : "integer"
        },
        "month": {
          "type": "string",
          "enum": ["jan", "feb", "mar", "apr", "may", "jun", "jul", "aug", "sep", "oct", "nov", "dec"]
        },
        "day": {
          "type" : "integer"
        }
      }
    },
    "time": {
      "type": "object",
      "properties": {
        "hour": {
          "type" : "integer"
        },
        "minute": {
          "type": "integer"
        },
        "seconds": {
          "type" : "integer"
        }
      }
    }
  }
}